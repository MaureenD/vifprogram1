    -o-transition: all 0.7s ease 0s;
    transition: all 0.7s ease 0s;
}
.mfp-fade.mfp-wrap {
    .mfp-content {
        opacity: 0;
        -webkit-transition: all 0.7s ease 0s;
        -moz-transition: all 0.7s ease 0s;
        -o-transition: all 0.7s ease 0s;
        transition: all 0.7s ease 0s;
    }
}
.mfp-fade.mfp-wrap.mfp-ready {
    .mfp-content {
        opacity: 1;
        -webkit-transition: all 0.7s ease 0s;
        -moz-transition: all 0.7s ease 0s;
        -o-transition: all 0.7s ease 0s;
        transition: all 0.7s ease 0s;
    }
}
.mfp-fade.mfp-wrap.mfp-removing {
    .mfp-content {
        opacity: 0;
        -webkit-transition: all 0.7s ease 0s;
        -moz-transition: all 0.7s ease 0s;
        -o-transition: all 0.7s ease 0s;
        transition: all 0.7s ease 0s;
    }
}
