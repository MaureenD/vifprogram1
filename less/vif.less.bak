    background-clip: padding-box;
    display: inline-block;
    .square-photo {
        -moz-background-clip: padding;
        -webkit-background-clip: padding-box;
        background-clip: padding-box;
        border-radius: @border-radius-base;
        -webkit-border-radius: @border-radius-base;
    }
}
.icon-outline {
    i {
        font-size: 2em;
